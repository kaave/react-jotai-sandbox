{
    "name": "react-typescript-todomvc-2022",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "run-s gen:* dev:*",
        "dev:vite": "vite",
        "gen": "run-p gen:*",
        "gen:css-types": "tcm src",
        "build": "vite build",
        "test": "cross-env NODE_ENV=test vitest",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
        "fix": "run-p fix:*",
        "fix:scripts": "pnpm prettier; eslint . --ext .ts,.tsx,.js,.jsx --fix",
        "prettier": "prettier --write \"**/*.+(json|yml|css|md|mdx)\"",
        "typecheck": "pnpm gen && tsc --noEmit"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version"
        ]
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.10.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^9.2.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@tsconfig/strictest": "^2.0.0",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.31",
        "@types/react-dom": "^18.0.11",
        "@types/styled-components": "^5.1.26",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "@vitejs/plugin-react": "^3.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.4",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-playwright": "^0.12.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-regexp": "^1.14.0",
        "eslint-plugin-testing-library": "^5.10.2",
        "eslint-plugin-unicorn": "^46.0.0",
        "eslint-plugin-vitest": "^0.0.57",
        "jsdom": "^21.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.7",
        "prettier-plugin-organize-imports": "^3.2.2",
        "rimraf": "^4.4.1",
        "typed-css-modules": "^0.7.2",
        "typescript": "^5.0.3",
        "vite": "^4.2.1",
        "vitest": "^0.29.8"
    }
}
